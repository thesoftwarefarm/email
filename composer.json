{
    "name": "tsfcorp/email",
    "description": "Laravel package for sending emails",
    "license": "MIT",
    "authors": [
        {
            "name": "Ionut Antohi",
            "email": "ionut.antohi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "laravel/framework": "~5.1|~6.0|~7.0",
        "aws/aws-php-sns-message-validator": "^1.5",
        "symfony/mailgun-mailer": "^5.1",
        "symfony/http-client": "^5.1",
        "symfony/google-mailer": "^5.1",
        "async-aws/ses": "^1.3",
        "symfony/amazon-mailer": "^5.1"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "TsfCorp\\Email\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TsfCorp\\Email\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TsfCorp\\Email\\EmailServiceProvider"
            ]
        }
    }
}
